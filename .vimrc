filetype plugin on
syntax on

set mouse=a
set tabstop=4
set autoindent
set background=dark
set clipboard=unnamed
set wildmenu
set wildmode=longest:full,full
set gdefault
set encoding=utf-8 nobomb
set ttyfast
set smartindent
set tabstop=4
set shiftwidth=4
set expandtab
set hlsearch
set ignorecase
set incsearch
"set lcs=tab:?\ ,trail:·,eol:¬,nbsp:_
"set list
set noerrorbells
set nostartofline
set title
set showcmd

source $VIMRUNTIME/mswin.vim
